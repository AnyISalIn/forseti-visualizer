{
	"name": "forseti-visualizer-api",
	"version": "2.0.0",
	"description": "Forseti Visualizer API to serve Forseti Based GCP content",
	"author": "Garrett Wong",
	"main": "server.js",
	"scripts": {
		"dev": "nodemon -w server --exec \"babel-node server",
		"build": "babel server -s -D -d dist",
		"serve": "nodemon -w server --exec \"babel-node server\"",
		"start": "npm run serve",
		"test": "babel server.js -d dist && mocha --recursive --exit --require @babel/register",
		"babel-version": "babel --version"
	},
	"eslintConfig": {
		"extends": "eslint:recommended",
		"parserOptions": {
			"ecmaVersion": 7,
			"sourceType": "module"
		},
		"env": {
			"node": true
		},
		"rules": {
			"no-console": 0,
			"no-unused-vars": 1
		}
	},
	"dependencies": {
		"@google-cloud/bigtable": "^0.15.0",
		"@google-cloud/kms": "^0.4.0",
		"@grpc/proto-loader": "^0.4.0",
		"axios": "^0.19.0",
		"body-parser": "^1.18.3",
		"braces": ">=2.3.1",
		"compression": "^1.7.3",
		"cors": "^2.8.4",
		"eslint-utils": "^1.4.1",
		"express": "^4.16.1",
		"express-session": "^1.15.6",
		"grpc": "^1.18.0",
		"lodash": ">=4.17.13",
		"lodash.merge": ">=4.6.2",
		"morgan": "^1.9.1",
		"mysql": "^2.16.0",
		"passport": "^0.4.0",
		"passport-google-oauth20": "^1.0.0",
		"pug": "^2.0.3",
		"resource-router-middleware": "^0.7.0",
		"session-file-store": "^1.2.0",
		"uuid": "^3.3.2"
	},
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/npm/npm.git"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.0",
		"@babel/core": "^7.7.0",
		"@babel/node": "^7.7.0",
		"@babel/preset-env": "^7.7.1",
		"@babel/register": "^7.7.0",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"eslint": "^5.3.0",
		"js-yaml": ">=3.13.1",
		"mocha": "^6.2.2",
		"nodemon": "^1.18.3",
		"sinon": "^7.5.0",
		"sinon-chai": "^3.3.0"
	}
}
